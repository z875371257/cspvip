.contarner{
  background: #F9F9F9;
}
/* 搜索框 */
.head-search{
  width:100%;
  height: 90rpx;
  background: #FFFFFF;
}
.search-top{
  width: 88%;
  margin:auto;
  font-size: 28rpx;  
  margin-top:30rpx;
  position: relative;
}
.search-top image{
  width:48rpx;
  height:48rpx;
  position: absolute;
  top:6rpx;
  right:10rpx;
  z-index: 99;
}
.search{
  width: 82%;
  padding:0 20rpx;
  height:60rpx;
  line-height: 60rpx;
  color:#aaaaaa;
  background:#F5F5F5;
  border-radius: 10rpx;
}
.dele{
  position: absolute;
  top:12rpx;
  right:5rpx;
}
/* 标签栏 */
.label{
  margin-top:10rpx;
  width: 100%;
  height:200rpx;
  background: #FFF;
}
.lab-text{
  width: 88%;
  padding-top:30rpx;
  font-size: 28rpx;
  color:#aaaaaa;
  margin:auto;
}
.lab-box{
  width: 88%;
  margin: auto;
  padding-top:30rpx;
}
.lab-box .lab-son:first-child{
  margin-left:0rpx;
}
.lab-son{
  background: #F9F9F9;
  border-radius: 10rpx;
  float:left;
  text-align: center;
  margin:10rpx;
}
.sons{
  font-size: 28rpx;
  padding:5rpx 15rpx;
  color:#aaaaaa;
}

/* 搜索结果栏 */
.search-result{
  background: #FFF;
  margin-top:10rpx;
  width:100%;
}
.res-box{
  width:88%;
  margin:auto
}
.list-li{
  height:120rpx;
  background: #FFFFFF;
  padding-top:30rpx;
  border-bottom:1px solid #E2E2E2;
}
.list-li:last-child{
  display: none
}
.z-tit{
  width:400rpx;
  height:40rpx;
  overflow: hidden;
}
.li-img{
  width:190rpx;
  height:90rpx;
  border:1px solid #BDBDBD;
  border-radius: 10rpx;
  float:left;
}
.li-img image{
  width:190rpx;
  height:90rpx;
}
.li-text{
  float:left;
  font-size:28rpx;
  width: 60%;
  margin-left:30rpx;
  padding-bottom: 25rpx;
}
.f-tit{
  margin-top:14rpx;
  width:100%;
  height:50rpx;
  overflow:hidden;
}
.f-tit .lab-son{
   margin-top:0; 
}
.f-tit .lab-son:first-child{
  margin-left:0;
}
/* 错误报表 */
.search-err{
  background: #FFF;
  margin-top:10rpx;
  width:100%;
}
.err-txt{
  margin: auto;
  width: 88%;
  color:#aaaaaa;
  font-size: 32rpx;
  padding-top:30rpx;
}